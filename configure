s%@DEBUG_FLAGS@%$DEBUG_FLAGS%g
s%@OPTO_FLAGS@%$OPTO_FLAGS%g
s%@WARN_FLAGS@%$WARN_FLAGS%g
s%@PH_VTM_OPTO_FLAGS@%$PH_VTM_OPTO_FLAGS%g
s%@DIC_CC@%$DIC_CC%g
s%@LD@%$LD%g
s%@AR@%$AR%g
s%@STD_INCLUDE@%$STD_INCLUDE%g
s%@MME_DIR@%$MME_DIR%g
s%@LINUX_AUDIO@%$LINUX_AUDIO%g
s%@LINK_LINUX_AUDIO@%$LINK_LINUX_AUDIO%g
s%@API_OBJ_IN@%$API_OBJ_IN%g
s%@ALL_LIB_OBJS@%$ALL_LIB_OBJS%g
s%@MME_COMPILE_FLAGS@%$MME_COMPILE_FLAGS%g
s%@DAPI_SRC_ALL@%$DAPI_SRC_ALL%g
s%@DECTALK_TTS_OBJS@%$DECTALK_TTS_OBJS%g
